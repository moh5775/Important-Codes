
import java.util.Scanner;

public class CF_A {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        String str = input.nextLine();
        String[] words = str.split(" ");
        StringBuffer rev = new StringBuffer();// to effiiently build the reverse string
        int i;
        /* words.length: Number of splitted words */
        for (i = words.length - 1; i >= 0; i--) {
            rev.append(words[i]).append(" ");//Append word of ith position with space to the StringBuffer 
        }
        System.out.println(rev);
    }
}
