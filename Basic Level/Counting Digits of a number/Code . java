import java.util.Scanner;

public class Basic_4_CountDigits {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n;
        System.out.print("n: ");
        n = input.nextInt();
        int count = 0;
        while (n != 0) {
            n = n / 10;
            count++;
        }
        System.out.print("Number of Digit: " + count);
    }
}
