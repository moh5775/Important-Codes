import java.util.Scanner;

public class Basic_10_FactorsOfAnyNumber {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n, i;
        System.out.print("Enter Number: ");
        n = input.nextInt();
        System.out.print("All Factors of " + n + " : ");
        for (i = 1; i < n; i++) {
            if (n % i == 0) {
                System.out.print(i + " ");
            }
        }
    }
}
